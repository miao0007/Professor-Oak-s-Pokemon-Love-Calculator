{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": false,
    "browser": false,
    "commonjs": false,
    "node": false
  },
  "extends": "prettier",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "array-callback-return": "error",
    "camelcase": "off",
    "curly": "error",
    "dot-notation": "off",
    "eqeqeq": "error",
    "implicit-arrow-linebreak": [
      "error",
      "beside"
    ],
    "indent": [
      "error",
      2
    ],
    "no-duplicate-case": "off",
    "no-else-return": "off",
    "no-empty": "off",
    "no-empty-function": "off",
    "no-extra-semi": "off",
    "no-func-assign": "off",
    "no-irregular-whitespace": "off",
    "no-multi-spaces": "off",
    "no-unreachable": "off",
    "no-unused-vars": "off",
    "no-var": "off",
    "prefer-arrow-callback": "off",
    "prefer-const": "off",
    "prettier/prettier": "off",
    "quotes": [
      "off",
      "double"
    ],
    "semi": [
      "off",
      "always"
    ],
    "vars-on-top": "off",
    "yoda": "off"
  }
}